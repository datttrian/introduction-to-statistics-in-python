{
    "git.autofetch": true,
    "gitdoc.enabled": true,
    "gitdoc.autoCommitDelay": 60000,
    "gitdoc.autoPushDelay": 0,
    "gitdoc.commitMessageFormat": "ccc, LLL d, y, h:mm a '+00:00'",
    "flake8.args": [
        "--ignore=E111,E114,E231,E402,E501"
    ],
    "files.insertFinalNewline": true
}
